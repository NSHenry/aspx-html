{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ASPX-HTML",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{ "include": "#tags" },
		{ "include": "#samples" },
		{
			"include": "source.aspx"
		},
		{
			"include": "text.html.basic"
		},{
			"name":"keyword.control.aspx",
			"match": "(<%[#@]?)|(%>)|(&&)|(eval|Eval)"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.aspx",
					"match": "\\b(if|while|for|return)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.aspx",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.aspx",
					"match": "\\\\."
				}
			]
		},
		"tags": {
			"patterns": [
				{
					"name": "entity.name.tag.aspx",
					"match": "\\b(ASP|TextBox|Buttom|Register|Page)\\b"
				},{
					"name":"entity.name.type",
					"match": "\\b(类型|Src|TagName|TagPrefix|Language|AutoEventWireup|CodeFile)\\b"
				}
			]
		},"samples":{
